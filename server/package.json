{
  "name": "server",
  "version": "1.2.0",
  "description": "Apache Benchmark testing server",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "uuid": "^9.0.0"
  }
}
